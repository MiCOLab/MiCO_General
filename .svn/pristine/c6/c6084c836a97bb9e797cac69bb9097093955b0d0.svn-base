* {
	margin: 0px;
	padding: 0px;
}
.content {
	margin: 21px 30px 0px 30px;
	padding: 21px 19px;
	/*background-color: #18161d;*/
	border-radius: 5px;
}
.serachText {
	color: #3DB481;
	margin: 0 auto;
}
.serachText span {
	color: #3DB481;
	font-family: arial;
	font-size: 13px;
}
.serachText img {
	height: 8px;
	width: 8px;
}
#wifi_ssid::-webkit-input-placeholder {
	color: #FFFFFF;
}
#wifi_psw::-webkit-input-placeholder {
	color: #3DB481;
}
#wifi_ssid:-moz-placeholder {
	color: #FFFFFF;
}
#wifi_psw:-moz-placeholder {
	color: #3DB481;
}
.p img {
	width: 75px;
	margin-top: 17.5px;
}
.p {
	text-align: center;
}
